<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="klase.Profesor" table="Profesor" schema="public" catalog="is1_projekat_baza">
        <id name="mbr">
            <column name="mbr" sql-type="bigint"/>
        </id>
        <property name="ime">
            <column name="ime" sql-type="text"/>
        </property>
        <property name="prezime">
            <column name="prezime" sql-type="text"/>
        </property>
        <property name="fondCasova">
            <column name="fond_casova" sql-type="bigint"/>
        </property>
        <property name="radniStaz">
            <column name="radni_staz" sql-type="bigint"/>
        </property>
        <property name="brojTelefona">
            <column name="broj_telefona" sql-type="text" not-null="true"/>
        </property>
        <property name="username">
            <column name="username" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>