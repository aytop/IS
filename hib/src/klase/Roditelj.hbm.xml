<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="klase.Roditelj" table="Roditelj" schema="public" catalog="is1_projekat_baza">
        <id name="idr">
            <column name="idr" sql-type="bigint"/>
        </id>
        <property name="ime">
            <column name="ime" sql-type="text"/>
        </property>
        <property name="prezime">
            <column name="prezime" sql-type="text"/>
        </property>
        <property name="username">
            <column name="username" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>