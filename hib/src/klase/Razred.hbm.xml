<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="klase.Razred" table="Razred" schema="public" catalog="is1_projekat_baza">
        <id name="idr">
            <column name="idr" sql-type="text"/>
        </id>
        <property name="godina">
            <column name="godina" sql-type="bigint"/>
        </property>
        <property name="odelenje">
            <column name="odelenje" sql-type="bigint"/>
        </property>
        <property name="razredniStaresina">
            <column name="razredni_staresina" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>